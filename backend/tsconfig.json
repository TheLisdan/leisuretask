    "plugins": [{ "transform": "typescript-transform-paths" }],
    "paths": {
      "@leisuretask/webapp/*": ["../webapp/*"]
    },
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "moduleDetection": "force",
    "outDir": "./dist"
  },
  "include": ["src"]
}
